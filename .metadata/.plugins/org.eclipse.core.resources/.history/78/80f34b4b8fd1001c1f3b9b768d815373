package worker;

public abstract class Worker implements Runnable  {

	public Worker(String name, int speed, Thread thread) {
		super();
		this.name = name;
		this.speed = speed;
		this.thread = thread;
	}

	String name;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getSpeed() {
		return speed;
	}
	public void setSpeed(int speed) {
		this.speed = speed;
	}

	int speed;
	public Thread thread;
	
	
}
